        </activity>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCa-P6oeT2WlkILaoMsZAK6QstDQ1ra-DI"/>
        <activity
            android:name=".UpdateDataActivity"
